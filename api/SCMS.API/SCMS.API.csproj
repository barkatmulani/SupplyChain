<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <AssemblyName>SCMS.API</AssemblyName>
    <RootNamespace>SCMS.API</RootNamespace>
    <OutputPath></OutputPath>
    <ApplicationIcon />
